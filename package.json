{
  "name": "starter-apps",
  "version": "1.0.0",
  "private": true,
  "dependencies": {
    "@aws-amplify/ui-react": "^0.2.22",
    "@monaco-editor/react": "^3.6.2",
    "accounting": "^0.4.1",
    "array-to-tree": "^3.3.2",
    "aws-amplify": "^3.3.2",
    "classnames": "^2.2.6",
    "dot-prop-immutable": "^2.1.0",
    "grommet": "^2.15.1",
    "grommet-icons": "^4.4.0",
    "history": "^5.0.0",
    "inflection": "^1.12.0",
    "lodash": "^4.17.20",
    "moment": "^2.27.0",
    "money": "^0.2.0",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-desc": "^4.1.2",
    "react-dom": "^16.13.1",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "3.4.3",
    "react-use": "^15.3.3",
    "styled-components": "^5.1.1",
    "timeago.js": "^4.0.2",
    "uuid": "^8.3.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.8.2",
    "@cypress/instrument-cra": "^1.3.2",
    "@testing-library/jest-dom": "^5.11.4",
    "@testing-library/react": "^11.1.0",
    "@testing-library/react-hooks": "^3.4.1",
    "@testing-library/user-event": "^12.1.8",
    "body-parser": "^1.19.0",
    "cypress": "^5.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-jest": "^23.18.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^5.1.0",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "gray-matter": "^4.0.2",
    "husky": "^4.2.5",
    "jest-extended": "^0.11.5",
    "lint-staged": "^10.2.11",
    "lowdb": "^1.0.0",
    "nodemon": "^2.0.5",
    "prettier": "^2.0.5",
    "react-test-renderer": "^16.13.1"
  },
  "scripts": {
    "build": "react-scripts build",
    "start": "react-scripts -r @cypress/instrument-cra start",
    "cypress": "cypress open",
    "test": "react-scripts test --env=jsdom --watchAll=false",
    "test-watch": "react-scripts test --env=jsdom",
    "coverage": "react-scripts test --env=jsdom  --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "format": "prettier --write src/**/*.js",
    "lint": "eslint src/**/*.js",
    "all:generate": "yarn data:generate && yarn samples:generate",
    "data:watch": "nodemon -e mdx --watch src --exec 'yarn data:generate'",
    "data:generate": "node tools/generate-data.js",
    "samples:generate": "node tools/generate-samples.js"
  },
  "eslintConfig": {
    "extends": "react-app",
    "globals": {
      "render": true,
      "renderContainer": true,
      "renderComponent": true,
      "cy": true,
      "context": true,
      "Cypress": true
    }
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "lint-staged": {
    "*.{js,md,css}": [
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn test"
    }
  },
  "nyc": {
    "exclude": "src/serviceWorker.js"
  }
}
